    <script type="text/template" id="settings-template">
        <div class="setting clearfix">
            <div>Lado de Barra lateral</div>
            <div id="sidebar-toggle" class="pull-left btn-group" data-toggle="buttons-radio">
                <% onRight = sidebar == 'right'%>
                <button type="button" data-value="left" class="btn btn-sm btn-default <%= onRight? '' : 'active' %>">Izq</button>
                <button type="button" data-value="right" class="btn btn-sm btn-default <%= onRight? 'active' : '' %>">Der</button>
            </div>
        </div>
        <div class="setting clearfix">
            <div>Barra Lateral</div>
            <div id="display-sidebar-toggle" class="pull-left btn-group" data-toggle="buttons-radio">
                <% display = displaySidebar%>
                <button type="button" data-value="true" class="btn btn-sm btn-default <%= display? 'active' : '' %>">Most</button>
                <button type="button" data-value="false" class="btn btn-sm btn-default <%= display? '' : 'active' %>">Ocul</button>
            </div>
        </div>
    </script>

    <script type="text/template" id="sidebar-settings-template">
